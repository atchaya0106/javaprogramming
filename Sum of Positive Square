import java.util.*;
public class Solution {
    public static void main(String[] args) {
     Scanner sc=new Scanner(System.in);
     int size=sc.nextInt();
     int a[]=new int[size];
        int sum=0;
        for(int i=0;i<size;i++)
        { 
            a[i]=sc.nextInt();
        }
        for(int i=0;i<size;i++){
           if(a[i]>=0)
            sum=(sum + (a[i]*a[i]));
          }
        System.out.println(sum);
    }
}
